/*
 * analyseur syntaxique  du langage exemple-expr:
 * expression arithmetique
 * (voir supports de cours)
 *
 */ 
package fr.ul.miage.exemple.generated;

/* la grammaire */



non terminal fichier, while, for, declaration, commentaire, doc, if, else, affect, retourne, afficher, lire, function, param, expr;
terminal TQ, PR, PD,NUM, PG, ACD, ACG, ET, OU, PLUS, FOIS, MOINS, DIV, NOM, AFFECTE, SI, SINON, TYPE, FONCTION, DP, RT, LIR, STRING, FIN, COM, FCOM, AFFICH, ENTREE;
/* 	TQ : Tant que ; 
	PR : Pour ; 
	PD : Parenth�se droite ; 
	PG : Parenth�se gauche ; 
	ACD : Accolade gauche ; 
	ACG : Accolade droite ; 
	DP : Double point ; 
	RT : Retourne */


start with fichier;
fichier ::= while fichier|
			for fichier|
			declaration fichier|
			commentaire|
			doc fichier|
			if fichier|
			else fichier|
			affect fichier|
			retourne fichier|
			afficher fichier|
			lire fichier|
			function fichier |
			FIN fichier |
			STRING fichier |
			FIN
	;
	
param ::= 	PG TYPE NOM PD |
			PG TYPE NOM TYPE NOM PD |
			PG PD;
			
while ::= TQ param ACG fichier ACD;

for ::= PR param ACG fichier ACD;

declaration ::= TYPE NOM |
				TYPE NOM AFFECTE STRING;

commentaire ::= COM fichier FCOM fichier;

if ::= SI PG STRING PD ACG fichier ACD;

else ::= SINON PG STRING PD ACG fichier ACD;

affect ::= NOM AFFECTE STRING;

retourne ::= RT STRING;

afficher ::= AFFICH PG STRING PD;

lire ::= LIR PG PD;

function ::= FONCTION TYPE STRING param ACG fichier ACD;
